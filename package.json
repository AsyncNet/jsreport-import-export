{
  "name": "jsreport-import-export",
  "version": "0.4.2",
  "description": "jsreport extension adding templates exporting and importing API",
  "scripts": {
    "test": "mocha test --timeout 5000 --exit && standard",
    "test:watch": "mocha test --watch",
    "build": "jsreport-studio-build",
    "prepublishOnly": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-import-export",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-import-export.git"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "keywords": [
    "jsreport",
    "export",
    "import"
  ],
  "dependencies": {
    "archiver": "2.1.1",
    "bluebird": "3.5.1",
    "in-publish": "2.0.0",
    "multer": "1.3.0",
    "stream-to-array": "2.3.0",
    "unzip2": "0.2.5"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.16.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "jsreport-core": "1.5.1",
    "jsreport-express": "1.2.4",
    "jsreport-fs-store": "1.2.4",
    "jsreport-images": "1.1.1",
    "jsreport-mongodb-store": "0.5.6",
    "jsreport-postgres-store": "0.3.0",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "1.2.2",
    "mocha": "5.0.0",
    "should": "13.2.1",
    "standard": "10.0.3",
    "supertest": "3.0.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
