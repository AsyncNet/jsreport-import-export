{
  "name": "jsreport-import-export",
  "version": "0.1.0",
  "description": "jsreport extension adding templates exporting and importing API",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "test:watch": "mocha test --watch",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-import-export",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-import-export.git"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "keywords": [
    "jsreport",
    "export",
    "import"
  ],
  "dependencies": {
    "archiver": "1.3.0",
    "bluebird": "3.4.6",
    "in-publish": "2.0.0",
    "multer": "1.2.1",
    "stream-to-array": "2.3.0",
    "unzip2": "0.2.5"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.12.2",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-babel": "4.0.0",
    "jsreport-core": "1.0.10",
    "jsreport-express": "1.0.2",
    "jsreport-fs-store": "1.0.7",
    "jsreport-images": "1.0.3",
    "jsreport-mongodb-store": "0.5.5",
    "jsreport-postgres-store": "0.1.4",
    "jsreport-studio-dev": "1.0.2",
    "jsreport-templates": "1.0.1",
    "mocha": "3.2.0",
    "should": "11.1.2",
    "standard": "8.6.0",
    "supertest": "2.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
